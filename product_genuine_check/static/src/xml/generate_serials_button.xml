<templates id="template" xml:space="preserve">
    <t t-name="product_genuine_check.ListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
          
            <!-- Button logic restricted to 'genuine.check' model -->
            <t t-if="model.root.resModel === 'genuine.check'">
                <button type="button" class="o_btn_generate_serials btn btn-primary" t-on-click="openWizard">
                    Generate Serials
                </button>
            </t>
        </xpath>
    </t>
</templates>


















